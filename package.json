{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:ts": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./src/migrate.ts",
    "migrate:js": "node ./dist/migrate.js",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "npm test -- --watchAll",
    "test:ci": "npm test -- --coverage",
    "test:cov": "npm run test:ci && coveralls < coverage/lcov.info",
    "test:unit": "npm test -- --watchAll -c jest-unit.config.ts",
    "test:request": "npm test -- --watchAll -c jest-request-config.js",
    "test:integration": "npm test -- --watchAll -c jest-integration.config.ts",
    "test:e2e": "npm test -- --watchAll -c jest-e2e.config.ts",
    "test:staged": "npm test -- --findRelatedTests"
  },
  "keywords": [],
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/chance": "^1.1.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.4",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.3.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
